    "husky": "^2.7.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "16.13.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "debug": false
        }
      ],
      "@babel/preset-typescript",
      "react-app"
    ],
    "plugins": [
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-optional-chaining"
    ]
  }
}
